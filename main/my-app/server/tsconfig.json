{
  "compilerOptions": {
    "target": "ES2022",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "lib": ["ESNext"]
  },
  "include": ["next-env.d.ts", "global.d.ts", "**/*.ts", "**/*.tsx", "static/decoder.worker.js"],
  "exclude": ["node_modules"]
}

